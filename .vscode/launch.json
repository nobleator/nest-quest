    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug 2 locations",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/app/bin/Debug/net7.0/app.dll",
            "args": ["a within 1000 m", "1600 Pennsylvania Avenue, Washington, DC", "10 Downing Street, London, UK",],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false
            },
        },
        {
            "name": "Debug tree",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/app/bin/Debug/net7.0/app.dll",
            "args": ["(a within 1 m or b within 2 m) and c within 3 m and d within 4 m",],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false
            },
        }
    ]
}